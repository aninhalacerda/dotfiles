[user]
	name = Aninha Lacerda
	email = alacerda@thoughtworks.com
[push]
  default = tracking
[alias]
  ci = commit
  cm = commit -m
  ap = add -p .
  a = !git add -A . && git status
  st = status
  br = branch
  pr = pull --rebase
  spr = !git stash && git pr && git stash pop
  co = checkout
  df = diff
  plog = log --graph --pretty=format:'%Cred%h%Creset %C(bold blue)<%an>%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)' --abbrev-commit
  l = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset' --branches
  sr = svn rebase
  sd = svn dcommit
[core]
  editor = vim
[color]
  ui = auto
[branch "master"]
  remote = origin
  merge = refs/heads/master
[apply]
  whitespace = fix
[merge]
  stat = true
[hub]
  protocol = https
[credential]
	helper = osxkeychain
